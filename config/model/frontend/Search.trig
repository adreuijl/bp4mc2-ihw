  config:SearchConceptenAppearance a elmo:TableAppearance;
    elmo:fragment [
      elmo:index 1;
      elmo:appliesTo "begrip";
      xhtml:link "begrip";
      elmo:name "SUBJECT";
      rdfs:label "Term"@nl;
    ];
    elmo:fragment [
      elmo:index 4;
      elmo:appliesTo "categorie_begrip";
      xhtml:link "/begrippen-per-categorie";
      elmo:name "categorie";
